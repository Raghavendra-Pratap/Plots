{
  "name": "unified-data-studio",
  "version": "1.0.0",
  "description": "Unified Data Studio - Advanced data processing and analysis platform",
  "main": "main.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "clsx": "^2.0.0",
    "duckdb": "^1.3.2",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0",
    "web-vitals": "^2.1.4",
    "python-shell": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "electron-pack-mac": "npm run build && electron-builder --mac",
    "electron-pack-win": "npm run build && electron-builder --win",
    "electron-pack-linux": "npm run build && electron-builder --linux"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.unifieddatastudio.app",
    "productName": "Unified Data Studio",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "backend/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "backend",
        "to": "backend"
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "unified.png"
    },
    "win": {
      "icon": "unified.png"
    },
    "linux": {
      "icon": "unified.png"
    }
  }
}
